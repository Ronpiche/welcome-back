{
  "name": "hub-back",
  "version": "0.0.1",
  "description": "the back end service for the HUB dashboard",
  "author": "Daveo",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "nest build",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start": "nest start",
    "start:prod": "node dist/src/main",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:inspect-config": "npx @eslint/config-inspector",
    "test:unit": "cross-env NODE_ENV=test jest --config configs/jest/jest-unit.config.ts",
    "test:unit:cov": "cross-env NODE_ENV=test && rimraf tests/unit/coverage && jest --config configs/jest/jest-unit.config.ts",
    "test:integration": "cross-env NODE_ENV=test jest --config ./configs/jest/jest-e2e.config.ts",
    "test:stryker": "cross-env NODE_ENV=test rimraf tests/stryker/coverage && stryker run configs/stryker/stryker.conf.js",
    "test:stryker:force": "cross-env NODE_ENV=test rimraf tests/stryker/incremental.json tests/stryker/coverage && stryker run configs/stryker/stryker.conf.js --force",
    "docker:run:env": "docker-compose -f ./configs/docker/docker-compose.yml up",
    "prepare": "husky",
    "script:create-git-branch": "bash scripts/create-git-branch.sh",
    "script:create-pull-request": "bash scripts/create-pull-request.sh"
  },
  "dependencies": {
    "@azure/msal-node": "^2.6.5",
    "@getbrevo/brevo": "^2.2.0",
    "@google-cloud/firestore": "^7.4.0",
    "@google-cloud/secret-manager": "^5.6.0",
    "@google-cloud/storage": "^7.11.1",
    "@jest/types": "^29.6.3",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^10.3.5",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/testing": "^10.3.5",
    "aws-jwt-verify": "^4.0.1",
    "axios": "^1.6.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "date-holidays": "^3.23.11",
    "express-session": "^1.18.1",
    "firebase-admin": "^12.2.0",
    "markdown-it": "^14.1.0",
    "memorystore": "^1.6.7",
    "msw": "^2.2.13",
    "nestjs-session": "^3.0.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.14",
    "openid-client": "^5.7.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/config-inspector": "^0.5.4",
    "@faker-js/faker": "^9.3.0",
    "@stryker-mutator/core": "^8.5.0",
    "@stryker-mutator/jest-runner": "^8.5.0",
    "@stryker-mutator/typescript-checker": "^8.5.0",
    "@stylistic/eslint-plugin": "^2.8.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "eslint-plugin-jest": "^28.8.3",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "msw": "^2.2.13",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "validate-branch-name": "^1.3.1"
  }
}
